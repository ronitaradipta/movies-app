{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "linebreak-style": "off",
    "no-else-return": "off",
    "comma-dangle": "off",
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "indent": "off",
    "no-console": "off",
    "camelcase": "off"
  }
}
